	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Terminal (integrated)",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "(Windows) Launch",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "python_d",
			"args": [
				"setup.py",
				"test",
				"--",
				"./tests/test_generic.py"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false
		}
	]
}
